# EIA Storage Accrual Engine Configuration
# Copy this file to .env and fill in your values

# EIA API Configuration
EIA_API_KEY=your_eia_api_key_here

# Data Processing Configuration
DEFAULT_START_DATE=2010-01-01
DEFAULT_END_DATE=2025-12-31
DEFAULT_SCENARIO_BAND=0.10

# Conversion Factors
BCF_TO_MMBTU_FACTOR=1037000.0

# Default Tariff Rates (per MMBtu)
DEFAULT_TARIFF_FIXED_MONTHLY=120000.0
DEFAULT_TARIFF_INJECTION=0.02
DEFAULT_TARIFF_WITHDRAWAL=0.03

# Default WACOG (Weighted Average Cost of Gas)
DEFAULT_WACOG_PER_MMBTU=3.25

# Estimator Weights (Method C: 0.5, Method A: 0.3, Method B: 0.2)
DEFAULT_ESTIMATOR_WEIGHTS=0.3,0.2,0.5

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Data Storage Paths
DATA_BRONZE_PATH=./data/bronze
DATA_SILVER_PATH=./data/silver
DATA_GOLD_PATH=./data/gold
OUTPUTS_PATH=./outputs
LOGS_PATH=./logs

# API Configuration
EIA_BASE_URL=https://api.eia.gov/v2
EIA_REQUEST_TIMEOUT=30
EIA_MAX_RETRIES=3
EIA_BACKOFF_FACTOR=2
